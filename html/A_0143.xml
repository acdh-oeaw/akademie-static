      <div type="page" n="" xml:id="d5">
                <pb n="" facs="#facs_3" xml:id="pag3"/>
        <div xml:id="d6">
          <p rendition="#?11-89-100--162-1492" facs="#facs_3_region_1568374520477_1151" xml:id="p15" xml:space="preserve">
            <lb facs="#facs_3_region_1568374520477_1151l1" xml:id="z69"/><choice><expan>Herrn</expan><abbr>Hrn<am>.</am></abbr></choice> <persName>Eduard Edler <choice><expan>von </expan><abbr>v<am>.</am></abbr></choice> Mayer</persName>
            <lb facs="#facs_3_region_1568374520477_1151l2" xml:id="z70"/>in <placeName>olmutz</placeName> zur 
            Heraus-<lb facs="#facs_3_region_1568374520477_1151l3" xml:id="z71"/>gabe des von ihm 
            vorge-<lb facs="#facs_3_region_1568374520477_1151l4" xml:id="z72"/>legten Schrift: "Des 
            <lb facs="#facs_3_region_1568374520477_1151l5" xml:id="z73"/><placeName>olmutz</placeName>er Bischofs <persName>Stanis-</persName><persName>laus Pawlowski</persName> 
            Ge-<lb facs="#facs_3_region_1568374520477_1151l7" xml:id="z74"/>gesandtschaftsreisen nach 
            <lb facs="#facs_3_region_1568374520477_1151l8" xml:id="z75"/><placeName>Polen</placeName>" einen 
            Unter-<lb facs="#facs_3_region_1568374520477_1151l9" xml:id="z76"/>stützungsbeitrag von 
            <lb facs="#facs_3_region_1568374520477_1151l10" xml:id="z77"/><num>300</num> <choice><expan>Gulden</expan><abbr>fr<am>.</am></abbr></choice> <choice><expan>Österreichische Währung</expan><abbr>Ö<am>.</am>W<am>.</am></abbr></choice> bei der 
            Ge-<lb facs="#facs_3_region_1568374520477_1151l11" xml:id="z78"/>sammt-Akademie zu 
            <lb facs="#facs_3_region_1568374520477_1151l12" xml:id="z79"/>beantragen, denselben 
            <lb facs="#facs_3_region_1568374520477_1151l13" xml:id="z80"/>anzuweisen, wenn das 
            <lb facs="#facs_3_region_1568374520477_1151l14" xml:id="z81"/>Werk in Drucke sollen,<lb facs="#facs_3_region_1568374520477_1151l15" xml:id="z82"/>das vorliegen wird, und 
            <lb facs="#facs_3_region_1568374520477_1151l16" xml:id="z83"/>gegen dem, daß dieser 
            <lb facs="#facs_3_region_1568374520477_1151l17" xml:id="z84"/>Unterstützung der 
            Aka-<lb facs="#facs_3_region_1568374520477_1151l18" xml:id="z85"/>demie Erwähnung gethan 
            <lb facs="#facs_3_region_1568374520477_1151l19" xml:id="z86"/>und von dem Werke 
            <lb facs="#facs_3_region_1568374520477_1151l20" xml:id="z87"/><num>25</num> Frei-Exemplare zur 
            <lb facs="#facs_3_region_1568374520477_1151l21" xml:id="z88"/>Verteilung unter die 
            <lb facs="#facs_3_region_1568374520477_1151l22" xml:id="z89"/>Mitglieder der Classe 
            <lb facs="#facs_3_region_1568374520477_1151l23" xml:id="z90"/>abgeliefert werden.<lb facs="#facs_3_region_1568374520477_1151l24" xml:id="z91"/>Die Classe ersucht 
            <lb facs="#facs_3_region_1568374520477_1151l25" xml:id="z92"/>demnach um Genehmigung 
