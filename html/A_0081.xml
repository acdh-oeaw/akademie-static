            <lb facs="#facs_1_region_1556098486191_455l9" xml:id="z17"/><persName>Petzval</persName>
            <lb facs="#facs_1_region_1556098486191_455l10" xml:id="z18"/><persName>Brücke</persName>
            <lb facs="#facs_1_region_1556098486191_455l11" xml:id="z19"/><persName>Seidl</persName>
            <lb facs="#facs_1_region_1556098486191_455l12" xml:id="z20"/><persName>Birk</persName>
