          <p rendition="#fc" facs="#facs_1_region_1556098479611_452" xml:id="p10" xml:space="preserve">
            <lb facs="#facs_1_region_1556098479611_452l1" xml:id="z24"/><persName>von Prechtl</persName>
            <lb facs="#facs_1_region_1556098479611_452l2" xml:id="z25"/><persName>Zippe</persName>
            <lb facs="#facs_1_region_1556098479611_452l3" xml:id="z26"/><persName>Partsch</persName>
            <lb facs="#facs_1_region_1556098479611_452l5" xml:id="z27"/><persName>Arneth</persName>
            <lb facs="#facs_1_region_1556098479611_452l6" xml:id="z28"/><persName>Stampfer</persName>
            <lb facs="#facs_1_region_1556098479611_452l8" xml:id="z29"/><persName>von Ettingshausen</persName>
            <lb facs="#facs_1_region_1556098479611_452l9" xml:id="z30"/><persName>Chmel</persName>
            <lb facs="#facs_1_region_1556098479611_452l10" xml:id="z31"/><persName>Redtenbacher</persName>
