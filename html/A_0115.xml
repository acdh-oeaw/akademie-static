        <div xml:id="d2">
          <p rendition="#l" facs="#facs_1_region_1562148392840_700" xml:id="p1" xml:space="preserve">
            <lb facs="#facs_1_region_1562148392840_700l1" xml:id="z1"/>A<num>115</num>.</p>
          <p rendition="#fc" facs="#facs_1_region_1562148394568_703" xml:id="p2" xml:space="preserve">
            <lb facs="#facs_1_region_1562148394568_703l1" xml:id="z2"/>Kaiserliche Akademie der Wissenschaften.</p>
          <p rendition="#fc" facs="#facs_1_region_1562148397769_706" xml:id="p3" xml:space="preserve">
            <lb facs="#facs_1_region_1562148397769_706l1" xml:id="z3"/>Protokoll</p>
          <p rendition="#fc" facs="#facs_1_region_1562148400960_709" xml:id="p4" xml:space="preserve">
            <lb facs="#facs_1_region_1562148400960_709l3" xml:id="z4"/>der Gesammtsitzung am <num>26</num>. November <num>1857</num>.</p>
          <p rendition="#?17-83-100--405-2410" facs="#facs_1_region_1562148409497_715" xml:id="p5" xml:space="preserve">
            <lb facs="#facs_1_region_1562148409497_715l1" xml:id="z5"/><choice><expan>Seine</expan><abbr>S<hi rend="super">ne</hi></abbr></choice> Excellenz der Präsident <choice><expan>Freiherr</expan><abbr>Freih<am>.</am></abbr></choice> von <persName>Baumgartner</persName>.<lb facs="#facs_1_region_1562148409497_715l2" xml:id="z6"/>Der Vice-Präsident <persName>von Karajan</persName>.<lb facs="#facs_1_region_1562148409497_715l3" xml:id="z7"/>Der Generalsecretär <persName>Schrötter</persName>.<lb facs="#facs_1_region_1562148409497_715l4" xml:id="z8"/>Der zweite Secretär <persName>Wolf</persName>.</p>
          <p rendition="#lc" facs="#facs_1_region_1562148403984_712" xml:id="p6" xml:space="preserve">
            <lb facs="#facs_1_region_1562148403984_712l1" xml:id="z9"/>Anwesend:</p>
          <p rendition="#fc" facs="#facs_1_region_1562148412273_718" xml:id="p7" xml:space="preserve">
            <lb facs="#facs_1_region_1562148412273_718l1" xml:id="z10"/>Die wirklichen Mitglieder</p>
          <p rendition="#l" facs="#facs_1_region_1562148416673_721" xml:id="p8" xml:space="preserve">
            <lb facs="#facs_1_region_1562148416673_721l1" xml:id="z11"/><persName>Zippe</persName>
