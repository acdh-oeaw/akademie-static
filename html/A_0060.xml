          <p rendition="#l" facs="#facs_1_region_1549011386878_60" xml:id="p2" xml:space="preserve">
            <lb facs="#facs_1_line_1549012531498_181" xml:id="z1"/>A <num>60</num>.</p>
          <p rendition="#fc" facs="#facs_1_region_1549011389503_63" xml:id="p3" xml:space="preserve">
            <lb facs="#facs_1_line_1549012538233_186" xml:id="z2"/>Kaiserliche Akademie der Wissenschaften.</p>
          <p rendition="#fc" facs="#facs_1_region_1549011392066_66" xml:id="p4" xml:space="preserve">
