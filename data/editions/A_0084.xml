        <div xml:id="d2">
          <p rendition="#l" facs="#facs_1_region_1556102128285_849" xml:id="p1" xml:space="preserve">
            <lb facs="#facs_1_region_1556102128285_849l1" xml:id="z1"/>A <num>84</num>.</p>
          <p rendition="#fc" facs="#facs_1_region_1556102132203_852" xml:id="p2" xml:space="preserve">
            <lb facs="#facs_1_region_1556102132203_852l1" xml:id="z2"/>Kaiserliche Akademie der Wissenschaften.</p>
          <p rendition="#fc" facs="#facs_1_region_1556102135417_855" xml:id="p3" xml:space="preserve">
            <lb facs="#facs_1_region_1556102135417_855l1" xml:id="z3"/>Protokoll</p>
          <p rendition="#fc" facs="#facs_1_region_1556102138193_858" xml:id="p4" xml:space="preserve">
            <lb facs="#facs_1_region_1556102138193_858l1" xml:id="z4"/>der Gesammtsitzung am <num>22</num>. November <num>1854</num>.</p>
          <p rendition="#?24-76-100--572-2425" facs="#facs_1_region_1556102148926_864" xml:id="p5" xml:space="preserve">
            <lb facs="#facs_1_region_1556102148926_864l3" xml:id="z5"/><choice><expan>Seine</expan><abbr>S<hi rend="super">ne</hi></abbr></choice> Excellenz der Präsident <choice><expan>Freiherr</expan><abbr>Freih<am>.</am></abbr></choice> <persName>von Baumgartner</persName>
            <lb facs="#facs_1_region_1556102148926_864l4" xml:id="z6"/>Der Vice-Präsident <persName>von Karajan</persName>.<lb facs="#facs_1_region_1556102148926_864l5" xml:id="z7"/>Der Generalsecretär <persName>Schrötter</persName>.<lb facs="#facs_1_region_1556102148926_864l6" xml:id="z8"/>Der zweite Secretär <persName>Wolf</persName>.</p>
          <p rendition="#lc" facs="#facs_1_region_1556102141501_861" xml:id="p6" xml:space="preserve">
            <lb facs="#facs_1_region_1556102141501_861l1" xml:id="z9"/>Anwesend:</p>
          <p rendition="#fc" facs="#facs_1_region_1556102151890_867" xml:id="p7" xml:space="preserve">
