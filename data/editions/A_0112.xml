            <lb facs="#facs_4_region_1562142939212_440l2" xml:id="z99"/>der Geschäftsordnung gemäß,<lb facs="#facs_4_region_1562142939212_440l3" xml:id="z100"/><choice><expan>Herrn</expan><abbr>Hrn<am>.</am></abbr></choice> <persName>Eugen Obermayer</persName> <choice><expan>provorischer</expan><abbr>prov<am>.</am></abbr></choice>
            <lb facs="#facs_4_region_1562142939212_440l4" xml:id="z101"/>Concepts-Official bei 
            <lb facs="#facs_4_region_1562142939212_440l5" xml:id="z102"/>der <choice><expan>kaiserlich-königlichen</expan><abbr>k<am>.</am> k<am>.</am></abbr></choice> <choice><expan>priviligirten</expan><abbr>priv<am>.</am></abbr></choice> Carl 
