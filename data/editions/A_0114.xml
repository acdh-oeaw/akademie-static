          <p rendition="#lc" facs="#facs_1_region_1562147463022_608" xml:id="p1" xml:space="preserve">
            <lb facs="#facs_1_region_1562147463022_608l1" xml:id="z1"/>A. <num>114</num>.</p>
          <p rendition="#fc" facs="#facs_1_region_1562147465599_611" xml:id="p2" xml:space="preserve">
            <lb facs="#facs_1_region_1562147465599_611l1" xml:id="z2"/>Kaiserliche Akademie der Wissenschaften.</p>
          <p rendition="#fc" facs="#facs_1_region_1562147467559_614" xml:id="p3" xml:space="preserve">
            <lb facs="#facs_1_region_1562147467559_614l1" xml:id="z3"/>Protokoll</p>
          <p rendition="#fc" facs="#facs_1_region_1562147469895_617" xml:id="p4" xml:space="preserve">
            <lb facs="#facs_1_region_1562147469895_617l1" xml:id="z4"/>der Gesammtsitzung am <num>29</num>. October <num>1857</num>.</p>
          <p rendition="#?20-80-100--493-2480" facs="#facs_1_region_1562147475839_623" xml:id="p5" xml:space="preserve">
            <lb facs="#facs_1_region_1562147475839_623l2" xml:id="z5"/><choice><expan>Seine</expan><abbr>S<hi rend="super">ne</hi></abbr></choice> Excellenz der Präsident <choice><expan>Freiherr</expan><abbr>Freih<am>.</am><choice><expan>von</expan><abbr/></choice></abbr></choice><choice><expan>von</expan><abbr>v<am>.</am></abbr></choice> <persName>Baumgartner</persName>
            <lb facs="#facs_1_region_1562147475839_623l3" xml:id="z6"/>Der Vice-Präsident von Karajan-<lb facs="#facs_1_region_1562147475839_623l4" xml:id="z7"/>Der Generalsecretär <persName>Schrötter</persName>.<lb facs="#facs_1_region_1562147475839_623l5" xml:id="z8"/>Der zweite Secretär <persName>Wolf</persName>.</p>
          <p rendition="#lc" facs="#facs_1_region_1562147472006_620" xml:id="p6" xml:space="preserve">
            <lb facs="#facs_1_region_1562147472006_620l1" xml:id="z9"/>Anwesend:</p>
          <p rendition="#fc" facs="#facs_1_region_1562147479606_626" xml:id="p7" xml:space="preserve">
            <lb facs="#facs_1_region_1562147479606_626l2" xml:id="z10"/><hi>Die wirklichen Mitglieder</hi></p>
          <p rendition="#l" facs="#facs_1_region_1562147483623_629" xml:id="p8" xml:space="preserve">
            <lb facs="#facs_1_region_1562147483623_629l1" xml:id="z11"/><persName>Zippe</persName>
            <lb facs="#facs_1_region_1562147483623_629l2" xml:id="z12"/><persName>Arneth</persName>
            <lb facs="#facs_1_region_1562147483623_629l3" xml:id="z13"/><persName>Chmel</persName>
            <lb facs="#facs_1_region_1562147483623_629l4" xml:id="z14"/><persName>Kreil</persName>
            <lb facs="#facs_1_region_1562147483623_629l5" xml:id="z15"/><persName>Unger</persName>
            <lb facs="#facs_1_region_1562147483623_629l7" xml:id="z16"/><persName>Jäger</persName>
            <lb facs="#facs_1_region_1562147483623_629l8" xml:id="z17"/><choice><expan>Freiherr</expan><abbr>Freih<persName/></abbr></choice><persName>von Münch</persName>
            <lb facs="#facs_1_region_1562147483623_629l9" xml:id="z18"/><persName>Redtenbacher</persName>
